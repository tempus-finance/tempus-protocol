{
  "private": true,
  "name": "tempus-protocol",
  "version": "0.0.0",
  "description": "Tempus Protocol Contracts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tempus-finance/tempus-protocol.git"
  },
  "author": "Tempus",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/tempus-finance/tempus-protocol/issues"
  },
  "homepage": "https://github.com/tempus-finance/tempus-protocol#readme",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.10",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "2.0.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.2.0",
    "@types/chai": "^4.2.17",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.1",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "decimal.js": "^10.3.1",
    "dotenv": "^10.0.0",
    "eth-ens-namehash": "^2.0.8",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.4.1",
    "hardhat": "^2.2.1",
    "hardhat-abi-exporter": "^2.2.1",
    "hardhat-deploy": "^0.8.9",
    "hardhat-gas-reporter": "^1.0.4",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.10",
    "prompt": "^1.2.0",
    "prompts": "^2.4.2",
    "solhint": "^3.3.4",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.20",
    "ts-node": "^10.0.0",
    "typechain": "^5.1.1",
    "typescript": "^4.2.4",
    "yahoo-finance": "^0.3.7"
  },
  "scripts": {
    "build": "npx hardhat compile",
    "clean": "rm -rf ./cache && rm -rf ./artifacts crytic-export echidna-corpus",
    "test": "yarn build && bash run_tests.sh",
    "test:yearn": "yarn build && bash run_tests.sh Yearn",
    "test:integration:lido": "yarn build && bash run_tests.sh integration-suite Lido",
    "test:integration": "yarn build && bash run_tests.sh integration-tests",
    "coverage": "npx hardhat coverage --testfiles \"test/**/*.ts\"",
    "coverage:integration": "cross-env HARDHAT_FORK=mainnet HARDHAT_FORK_NUMBER=13281002 npx hardhat coverage --testfiles \"integration-tests/**/*.ts\"",
    "solhint": "solhint \"contracts/**/*.sol\"",
    "lint": "prettier --write \"contracts/**/*.sol\"",
    "lint-check": "prettier --check \"contracts/**/*.sol\"",
    "start-local": "npx hardhat node",
    "start-local:fork:fantom": "cross-env HARDHAT_FORK=fantom npx hardhat node",
    "start-local:fork:mainnet": "cross-env HARDHAT_FORK=mainnet npx hardhat node",
    "deploy-local:fork:mainnet": "cross-env HARDHAT_FORK=mainnet npx hardhat run scripts/deploy.local.fork.ts --network localhost",
    "deposit-local:fork:mainnet": "cross-env HARDHAT_FORK=mainnet npx hardhat run scripts/deposit.local.fork.ts --network localhost",
    "deploy-ci:fork:mainnet": "cross-env HARDHAT_FORK=mainnet npx hardhat run scripts/deploy.local.fork.ts",
    "deploy-local": "npx hardhat run scripts/deploy.local.ts --network localhost",
    "deploy-ci": "npx hardhat run scripts/deploy.local.ts",
    "deploy:controller": "npx hardhat run scripts/deploy.controller.ts",
    "deploy:pool:config": "npx hardhat run scripts/deploy.pool.config.ts",
    "deploy:stats": "npx hardhat run scripts/deploy.stats.ts",
    "deploy:vault": "npx hardhat run scripts/deploy.vault.ts",
    "verify:pool:lido": "cross-env TEMPUS_POOL_CONTRACT=LidoTempusPool npx hardhat run scripts/verify.pool.ts",
    "verify:pool:yearn": "cross-env TEMPUS_POOL_CONTRACT=YearnTempusPool npx hardhat run scripts/verify.pool.ts",
    "verify:pool:rari": "cross-env TEMPUS_POOL_CONTRACT=RariTempusPool npx hardhat run scripts/verify.pool.ts",
    "increase-time": "npx hardhat run scripts/increaseTime.local.ts --network localhost",
    "mythx-install": "pip install mythx-cli",
    "mythx-amm": "mythx --ci --yes analyze --wait --solc-version 0.8.10 contracts/amm/TempusAMM.sol",
    "mythx-pool": "mythx --ci --yes analyze --wait --solc-version 0.8.10 contracts/TempusController.sol contracts/pools/AaveTempusPool.sol contracts/pools/CompoundTempusPool.sol contracts/pools/LidoTempusPool.sol contracts/pools/RariTempusPool.sol contracts/pools/YearnTempusPool.sol",
    "mythx-misc": "mythx --ci --yes analyze --wait --solc-version 0.8.10 contracts/stats/Stats.sol contracts/token/TempusToken.sol contracts/token/vesting/ERC20Vesting.sol contracts/PositionManager.sol contracts/otc/TempusOTC.sol",
    "slither": "slither .",
    "echidna-test:pool:lido": "echidna-test ./ --contract EchidnaLidoTempusPool --config echidna.config.yaml --format text",
    "echidna-test:pool:yearn": "echidna-test ./ --contract EchidnaYearnTempusPool --config echidna.config.yaml --format text",
    "echidna-test-assertion:pool:lido": "echidna-test ./ --contract EchidnaLidoTempusPool --config echidna.config.yaml --test-mode assertion --format text",
    "echidna-test-assertion:pool:yearn": "echidna-test ./ --contract EchidnaYearnTempusPool --config echidna.config.yaml --test-mode assertion --format text"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.1.0"
  }
}
